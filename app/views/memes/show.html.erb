<div class="meme_show">
<h1><%= @meme.name %></h1>
<p>Created by: <%= @meme.user.username %></p>
<img src="<%= @meme.img_url%>">

<%= render "reviews/new" %>

<h3>Reviews of <%= @meme.name %>:</h3>
<ul class="reviews">
  <% @reviews.each do |review| %>
    <div class="review">
      <h6> Posted by: <%= review.user.username %> </h6>
      <li><% review.rating.times do %>♨️<% end %></li>
      <li class="profile_container"><img class="profile_thumb" src="<%= review.user.avatar %>"></li>
      <li><%= review.body %></li>
    </div>
  <% end %>
</ul>
</div>
